<!DOCTYPE html>
<html>
<head>
  <title>Cat Fight Club</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <%= render 'shared/header' %>
      </div>
    </div>
    <div class="row">    
      <div class="col-md-2">	
        <%= render 'shared/sidebar' %>
      </div>
      <div class="col-md-10">
        <%= yield %>
      </div>
    </div>
  </div>

</body>
</html>
